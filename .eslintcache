{ children }) => {\n  const [messages, setMessages] = useState<ToastMessage[]>([]);\n\n  const addToast = useCallback(({ title, description, type }: Omit<ToastMessage, 'id'>) => {\n    const id = uuid();\n\n    const toast = {\n      id,\n      title,\n      description,\n      type,\n    };\n\n    setMessages((state) => [...state, toast]);\n  }, []);\n\n  const removeToast = useCallback((id: string) => {\n    setMessages((state) => state.filter((message) => message.id !== id));\n  }, []);\n\n  return (\n    <ToastContext.Provider value={{ addToast, removeToast }}>\n      {children}\n      <ToastContainer messages={messages} />\n    </ToastContext.Provider>\n  );\n};\n\nexport const useToast = (): ToastContextData => {\n  const context = useContext(ToastContext);\n\n  if (!context) throw new Error('useToast must be used within an ToastProvider');\n\n  return context;\n};\n","/home/rofran/Development/GoStack-4/gobarber-web/src/services/api.ts",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/pages/SignUp/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/pages/SignIn/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/pages/Dashboard/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/pages/SignUp/styles.ts",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/pages/SignIn/styles.ts",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/utils/getValidationErrors.ts",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/components/ToastContainer/index.tsx",["132","133"],"import React from 'react';\nimport { useTransition } from 'react-spring';\n\nimport { ToastMessage } from '../../hooks/Toast';\n\nimport Toast from './toast/index';\n\nimport * as S from './styles';\n\ninterface ToastContainerProps {\n  messages: ToastMessage[];\n}\n\nconst ToastContainer: React.FC<ToastContainerProps> = ({ messages }) => {\n  const messagesWithTransitions = useTransition(messages, {\n    from: { right: '-120%', opacity: 0 },\n    enter: { right: '0%', opacity: 1 },\n    leave: { right: '-120%', opacity: 0 },\n  });\n\n  return (\n    <S.Container>\n      {messagesWithTransitions((style, item) => (\n        <Toast key={item.id} style={style} toast={item} />\n      ))}\n    </S.Container>\n  );\n};\n\nexport default ToastContainer;\n","/home/rofran/Development/GoStack-4/gobarber-web/src/components/ToastContainer/styles.ts",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/components/input/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/components/button/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/components/ToastContainer/toast/index.tsx",["134"],"import React, { useEffect } from 'react';\nimport { FiAlertCircle, FiXCircle, FiCheckCircle, FiInfo } from 'react-icons/fi';\n\nimport { useToast, ToastMessage } from '../../../hooks/Toast';\n\nimport * as S from './styles';\n\ninterface ToastProps {\n  toast: ToastMessage;\n  // eslint-disable-next-line @typescript-eslint/no-explicit-any\n  style: any;\n}\n\nconst icons = {\n  info: <FiInfo size={24} />,\n  error: <FiAlertCircle size={24} />,\n  success: <FiCheckCircle size={24} />,\n};\n\nconst Toast: React.FC<ToastProps> = ({ toast, style }) => {\n  const { removeToast } = useToast();\n\n  useEffect(() => {\n    const timer = setTimeout(() => {\n      removeToast(toast.id);\n    }, 3000);\n\n    return () => {\n      clearTimeout(timer);\n    };\n  }, [removeToast, toast.id]);\n\n  return (\n    <S.Container style={style} type={toast.type} hasDescription={Number(!!toast.description)}>\n      {icons[toast.type || 'info']}\n\n      <div>\n        <strong>{toast.title}</strong>\n        {toast.description && <p>{toast.description}</p>}\n      </div>\n\n      <button type=\"button\" onClick={() => removeToast(toast.id)}>\n        <FiXCircle size={16} />\n      </button>\n    </S.Container>\n  );\n};\n\nexport default Toast;\n","/home/rofran/Development/GoStack-4/gobarber-web/src/components/input/st