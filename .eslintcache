"/home/rofran/Development/GoStack-4/gobarber-web/src/styles/global.ts",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/routes/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/hooks/index.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/routes/Route.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/hooks/Auth.tsx",[],"/home/rofran/Development/GoStack-4/gobarber-web/src/hooks/Toast.tsx",["118"],"import React, { createContext, useContext, useCallback, useState } from 'react';\nimport { uuid } from 'uuidv4';\n\nimport ToastContainer from '../components/ToastContainer';\n\ninterface ToastContextData {\n  addToast: (message: Omit<ToastMessage, 'id'>) => void;\n  removeToast: (id: string) => void;\n}\n\nexport interface ToastMessage {\n  id: string;\n  type?: 'success' | 'error' | 'info';\n  title: string;\n  description?: string;\n}\n\nconst ToastContext = createContext<ToastContextData>({} as ToastContextData);\n\nexport const ToastProvider: React.FC = ({ children }) => {\n  const [messages, setMessages] = useState<ToastMessage[]>([]);\n\n  const addToast = useCallback(({ title, description, type }: Omit<ToastMessage, 'id'>) => {\n    const id = uuid();\n\n    const toast = {\n      id,\n      title,\n      description,\n      type,\n    };\n\n    setMessages((state) => [...state, toast]);\n  }, []);\n\n  const removeToast = useCallback((id: string) => {\n    setMessages((state) => state.filter((message) => message.id !== id));\n  }, []);\n\n  return (\n    <ToastContext.Provider value={{ addToast, removeToast }}>\n      {children}\n      <ToastContainer messages={messages} />\n    </ToastContext.Provider>\n  );\n};\n\nexport const useToast = (): ToastContextData => {\n  const context = useContext(ToastContext);\n\n  if (!context) throw new Error('useToast must be used within an ToastProvider');\n\n  return context;\n};\n","/home/rofran/Development/GoStack-4/gobarber-web/src/services/api.ts",[],"/home/rofran/Development/Go